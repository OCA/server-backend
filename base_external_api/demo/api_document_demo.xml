<odoo>

    <record id="api_document_partners_demo" model="apicli.document">
        <field name="name">Demo Customer Master Data Template</field>
        <field name="code">DemoCustomerMaster</field>
        <field name="model_id" ref="base.model_res_partner" />
        <field name="file_type">xml</field>
        <field
            name="endpoint"
        >/demoapi/{{o.company_id.id or o.env.company.id}}/customers</field>
        <field
            name="template_text"
            type="char"
            file="base_external_api/static/xml/DemoCustomerMaster.xml"
        />
    </record>

    <record id="scan_queue_message_process" model="ir.cron">
        <field name="name">Process of scan queue message</field>
        <field name="model_id" ref="base_external_api.model_apicli_message" />
        <field name="state">code</field>
        <field name="code">model.scan_queue_process()</field>
        <field name="interval_number">5</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False" />
    </record>

</odoo>
